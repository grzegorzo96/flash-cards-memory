---
import AppLayout from "@/layouts/AppLayout.astro";
import FlashcardFormPage from "@/components/flashcards/FlashcardFormPage";

const url = new URL(Astro.request.url);
const deckId = url.searchParams.get("deckId");
---

<AppLayout title="Utwórz fiszkę - FlashCardMemory" user={Astro.locals.user}>
  <FlashcardFormPage deckId={deckId} client:load />
</AppLayout>
